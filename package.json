{
  "name": "mcp-server",
  "version": "1.0.0",
  "description": "Simple MCP server for Google Cloud deployment",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:stdio": "node dist/mcp-stdio-server.js",
    "start:hybrid": "node dist/mcp-hybrid-server.js",
    "start:http": "MCP_MODE=http node dist/mcp-hybrid-server.js",
    "start:stdio-only": "MCP_MODE=stdio node dist/mcp-hybrid-server.js",
    "dev": "ts-node --esm src/index.ts",
    "dev:stdio": "ts-node --esm src/mcp-stdio-server.ts",
    "dev:hybrid": "ts-node --esm src/mcp-hybrid-server.ts",
    "test": "jest"
  },
  "keywords": ["mcp", "model-context-protocol", "google-cloud"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 